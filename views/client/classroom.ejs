    <script>
        let flag = '<%= user.theme%>'
        // theme create 
        let menu = document.querySelectorAll("#main,.theme-icon,.nav,body")
        let card = document.querySelectorAll(".card-pg")
        let discord = document.querySelectorAll(".discord")
            if(flag == 0){
                menu.forEach(elem=>{
                    elem.classList.remove("bg-slate-900")
                    elem.classList.remove("text-white")
                    elem.classList.remove("fill-white")
                    elem.classList.remove("stroke-white")
                    elem.classList.add("fill-black")
                    elem.classList.add("stroke-black")
                    elem.classList.add("bg-white")
                    elem.classList.add("text-black")
                })
                card.forEach(elem=>{
                    elem.classList.remove("bg-slate-800")
                    elem.classList.add("bg-slate-200")
                })
                discord.forEach(elem=>{
                    elem.classList.remove("bg-slate-600")
                    elem.classList.add("bg-slate-400")
                })
                
                
                flag = 1
            }else{
                menu.forEach(elem=>{
                    elem.classList.add("bg-slate-900")
                    elem.classList.add("text-white")
                    elem.classList.add("fill-white")
                    elem.classList.add("stroke-white")
                    elem.classList.remove("fill-black")
                    elem.classList.remove("stroke-black")
                    elem.classList.remove("bg-white")
                    elem.classList.remove("text-black")
                })
                card.forEach(elem=>{
                    elem.classList.add("bg-slate-800")
                    elem.classList.remove("bg-slate-200")
                })
                discord.forEach(elem=>{
                    elem.classList.add("bg-slate-600")
                    elem.classList.remove("bg-slate-400")
                })
                
                flag = 0
            }
    </script>
