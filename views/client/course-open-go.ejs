    <script src="https://vjs.zencdn.net/8.6.1/video.min.js"></script>
    <script>
        const player = videojs('vid1', {});

        let flag = '<%= user.theme%>'
        // theme create 
        let menu = document.querySelectorAll("#main,.theme-icon,.nav,input,body")
        let card = document.querySelectorAll(".card-video")
        let cardLink = document.querySelectorAll(".share-link-pg a")
        let des = document.querySelector(".des")

        document.addEventListener("DOMContentLoaded",()=>{

            if(flag == 0){
                menu.forEach(elem=>{
                    elem.classList.remove("bg-slate-900")
                    elem.classList.remove("text-white")
                    elem.classList.remove("fill-white")
                    elem.classList.remove("stroke-white")
                    elem.classList.add("fill-black")
                    elem.classList.add("stroke-black")
                    elem.classList.add("bg-white")
                    elem.classList.add("text-black")
                })
                card.forEach(elem=>{
                    elem.classList.remove("hover:bg-slate-950")
                    elem.classList.add("hover:bg-slate-200")
                })
                cardLink.forEach(elem=>{
                    elem.classList.remove("bg-slate-600")
                    elem.classList.add("bg-slate-300")
                })
                des.classList.remove("bg-slate-700")
                des.classList.add("bg-slate-200")
                flag = 1
            }
            else{
                menu.forEach(elem=>{
                    elem.classList.add("bg-slate-900")
                    elem.classList.add("text-white")
                    elem.classList.add("fill-white")
                    elem.classList.add("stroke-white")
                    elem.classList.remove("fill-black")
                    elem.classList.remove("stroke-black")
                    elem.classList.remove("bg-white")
                    elem.classList.remove("text-black")
                })
                card.forEach(elem=>{
                    elem.classList.add("hover:bg-slate-950")
                    elem.classList.remove("hover:bg-slate-200")
                })
                cardLink.forEach(elem=>{
                    elem.classList.add("bg-slate-600")
                    elem.classList.remove("bg-slate-300")
                })
                des.classList.add("bg-slate-700")
                des.classList.remove("bg-slate-200")
                flag = 0
            }
    
        })
    </script>
