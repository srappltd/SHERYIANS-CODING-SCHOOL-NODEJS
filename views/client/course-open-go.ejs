    <link href="https://vjs.zencdn.net/8.6.1/video-js.css" rel="stylesheet" />
    <style>
        body::-webkit-scrollbar { 
            display: none; 
        }
        .page-left{
            overflow-y: auto;
            /* background-color: aqua; */
            height: calc(90vh);
            scrollbar-width: none;
        }
        .page-left::-webkit-scrollbar{
            display: none;
        }
        .all{
            overflow-y: auto;
            /* background-color: aqua; */
            height: calc(70vh);
            scrollbar-width: none;
        }
        .all::-webkit-scrollbar{
            display: none;
        }
        @media (min-width:320px) and (max-width:700px) {
            .page-left{
                overflow-y: hidden;
                /* background-color: aqua; */
                height: calc(90vh);
                scrollbar-width: none;
                height: fit-content;
            }
            .all{
                overflow-y: hidden;
                /* background-color: aqua; */
                height: fit-content;
                scrollbar-width: none;
            }
        }
    </style>
